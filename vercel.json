{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/jira-ticket.html",
      "dest": "/jira-ticket.html"
    },
    {
      "src": "/confluence-page.html",
      "dest": "/confluence-page.html"
    },
    {
      "src": "/analysis.html",
      "dest": "/analysis.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
